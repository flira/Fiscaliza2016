/*
	[2.3]Posições,alinhamentos e floats
	_________________________________________________________
*/
div#menuar {
	position:absolute
}
sub,sup,div#listaacess,div#preconteudo,div#acessorapido,.borda,.titulo,.degrade,div.aba ul a   {
	position:relative
}
body,div.aba ul a,div#conteudo,div#fundoazul,div#fundoazul,div#fundorodape,#bannerrotativo {
	text-align:center
}
#subpagina .semborda.modular>.largura1 {
	text-align:right
}
body *,nav *,ul[role=navigation] *,div#fundorodape *,div#parcerias>div ul{
	text-align:left
}
div.grid,.modular,#subpagina .semborda.modular>.largura2,#prerodape ul.mapasite {
	text-align:justify
}
div#loginintegrado a,div.aba ul a,p#contato {
	vertical-align:middle
}
div.grid>*,div#parcerias li,.modular>div,ul.mapasite li  {
	vertical-align:text-top
}
.largura1.altura4,div#idiomas,a#logocabecalho,a#logofooter,address,div#acessibilidade,div#acessibilidade li,form#frmPesquisaPortal,#btpesquisa,div.aba ul li,ul#breadcrumb,ul#breadcrumb li,semborda,.borda img,.semborda>div,#slider,#slider li,#listaredes li,div#parcerias>div h1,.sitemap {
	float:left
}
div#loginintegrado,div#cabecalhodir,div#acessorapido {
	float:right
}
div#sobre nav{
	float:right !important
}
div#menuar>ul.mapasite>li,.semborda>div {
	float:none
}

