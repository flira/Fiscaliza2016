/*	[5]Media querries
__________________________________________________________________*/
@media screen and (max-width:945px) {
	div#parcerias>div{height:auto}
	div#parcerias ul{margin-left:100px}
	div#parcerias li {margin:7px 10px}
	div#cabecalho {padding:0}
	div#parcerias h1 {padding:45px 15px}
	#home div#preconteudo {height:66px}
	#home div#acessorapido {
		float:none;
		width:100%;
		margin:0 auto 10px	
	}
	p#contato,div#cabecalho>div {padding:0 10px}
	a#logofooter {margin:9px 7px 0}
	.altura1,.altura2 {min-height:130px}
}
@media screen and (max-width:780px) {
	#home .largura1 {width:49%}
	#home .altura4.largura1,#home .largura2,#home .largura3 {width:99.7%}
	.altura4.largura1{max-height:none}
	div#parcerias h1 {
			float:none !important;
			padding:15px;
			margin:-15px -15px 15px
		}
	div#parcerias ul {margin-left:-5px;text-align:center!important}
	a#logocabecalho {width:315px;height:44px;margin-top:8px;background-size:731px 342px;background-position:-412px -91px}
	div#cabecalho {height:120px;background-position:0 0}
	div#cabecalhodir {margin:10px 0;width:230px;text-align:right;float:right}
	div#cabecalhodir div,div#cabecalhodir nav{display:inline-block}
	div#acessibilidade {float:none}
	#home div#preconteudo {height: 110px}
	div.aba ul li{
			margin:2px .5%;
			width:49%;
		}
	div.aba ul a{
			display:block;
			height:auto;
			padding:5px 0;
			width:100%;
			z-index:0;		
		}
	#textoPesquisa {border:solid 1px}
}
@media screen and (max-width:590px) {
	#home .modular.largura3>div,#home .modular.largura3>div{width:90%;border:none}
    a#logocabecalho {width:75px;height:55px;margin-top:6px;background-size:975px 456px;background-position:-548px -121px}
}
@media screen and (max-width:440px) {
	#home div#preconteudo {height:175px}
	div.aba ul li{
			margin:1px 0;
			width:99%;
		}
	#home .largura1 {width:100%}
	.altura1,.altura2 {min-height:0}
	.borda>div,.degrade>div,.borda>span,.degrade>span {
			padding: 16px 8px 20px
		}
	input#textoPesquisa {width:175px}
	div#cabecalhodir {width:205px}
	div#loginintegrado {min-width: 200px}
}

