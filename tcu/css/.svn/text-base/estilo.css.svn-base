/*	[0]Definções Gerais

	[0.1]reset de navegador
__________________________________________________________________*/
* {
	font:1em/1.35 sans-serif;
	font-family: "Trebuchet MS",Helvetica,Arial,sans-serif!important;
	border:none;
	color:#444;
	font-style:normal;
	text-align:left;
	margin:0;
	padding:0
}
html {
	min-width:323px
}
article,aside,details,figcaption,figure,footer,header,hgroup,main,nav,section,summary {
	display:block
}
audio,canvas,video {
	display:inline-block
}
audio:not([controls]) {
	display:none;
	height:0
}
[hidden],template {
	display:none
}
html {
	-ms-text-size-adjust:100%;
	-webkit-text-size-adjust:100%
}
a {
	background:transparent
}
a:focus {
	outline:thin dotted
}
a:active,a:hover {
	outline:0
}
b,strong {
	color:#333
}
dfn,i,em {
	font-style:italic
}
hr {
	-moz-box-sizing:content-box;
	box-sizing:content-box;
	height:0
}
mark {
	background:#ff0;
	color:#000
}
code,kbd,pre,samp {
	font-family:monospace,serif;
	font-size:1em
}
pre {
	white-space:pre-wrap
}
q {
	quotes:\201C \201D \2018 \2019
}
small {
	font-size:80%
}
sub,sup {
	font-size:75%;
	line-height:0;
	vertical-align:baseline
}
sup {
	top:-.5em
}
sub {
	bottom:-.25em
}
img {
	border:0
}
svg:not(:root) {
	overflow:hidden
}
figure {
	margin:0
}
fieldset {
	border:1px solid silver;
	margin:0 2px;
	padding:.35em .625em .75em
}
button,input,select,textarea {
	font-family:inherit;
	font-size:100%;
	margin:0
}
button,input {
	line-height:normal
}
button,select {
	text-transform:none
}
button,html input[type=button],input[type=reset],input[type=submit] {
	-webkit-appearance:button;
	cursor:pointer
}
button[disabled],html input[disabled] {
	cursor:default
}
input[type=checkbox],input[type=radio] {
	box-sizing:border-box;
	padding:0
}
input[type=search] {
	-webkit-appearance:textfield;
	-moz-box-sizing:content-box;
	-webkit-box-sizing:content-box;
	box-sizing:content-box
}
input[type=search]::-webkit-search-cancel-button,input[type=search]::-webkit-search-decoration {
	-webkit-appearance:none
}
textarea {
	overflow:auto;
	vertical-align:top
}
table {
	border-collapse:collapse;
	border-spacing:0
}
legend,button::-moz-focus-inner,input::-moz-focus-inner {
	border:0;
	padding:0
}

/*
	[1]Especificações tipográficas
	
	[1.1]Tamanho das fontes e margens de texto
	_________________________________________________________
*/
body {
	font-size:.8125em;
}
table,span,div,input,select,textarea,td,a,pre {
	font-size: inherit;
}
h1 {
	font-size:1.35em;
	margin:1.35em 0 1em
}
h2 {
	font-size:1.25em;
	margin:1.25em 0 .875em
}
div#prerodape h1{
	font-size:1.2em
}
h3,.titulo {
	font-size:1.175em
}
th,
#subpagina.noticia article .datapub {
	font-size:1.1em
}
h3,h4,h5,h6,a,dd,li,p,span,div#menuar>ul.mapasite>li>ul>li>a,div#menuar>ul.mapasite h1{
	font-size:1em
}
div#cabecalho,
ul.aba li,
ul#breadcrumb,
div#acessorapido,
.borda a>p,
.degrade a>p,
div.menu,
div#prerodape,
p#contato{
	font-size:.85em
}
#btnar {
	font-size:.74em	
}
.textholder {
	font-size:small
}
/*
	[1.2]Margens e preenchimentos tipográficos adicionais
	_________________________________________________________
*/
#frmPesquisaPortal input[type=text] {
	margin-left:20px;
	padding:3px 5px
}
h3,h4,h5,h6 {
	margin:1em 0 .75em
}
.titulo h1 {
	margin:.9em;	
}

/*
	[1.3]Pesos
	_________________________________________________________
*/
ul#breadcrumb li>a  {
	font-weight:400
}
div.aba ul a,ul#breadcrumb li,h1,h2,h3,h4,b,strong{
	font-weight:700
}
div.menu div a.altocontraste.ativada,div.menu div a.altocontraste:active{
	font-weight:700!important
}

/*
	[1.4]Altura das linhas e indentações
	_________________________________________________________
*/
h1,h2,h3,h4,h5,h6,div#menuar>ul.mapasite h1 {
	line-height:1!important
}
div#menuar>ul.mapasite h1,ul.mapasite>li>ul>li>a{
	line-height:1.1
}
nav *,ul[role=navigation] *{
	line-height:1.5
}
div.aba ul a{
	line-height:1.7
}
nav *,ul[role=navigation] *{
	text-indent:0
}

/*
	[1.5]Cor das fontes
	_________________________________________________________
*/
a {color:darkBlue}
.verde h1,.verde h1>a{color:green!important}
.amarelo h1,.amarelo h1>a{color:#FFA200!important}
.azul h1,.azul h1>a,#tituloredes,div#parcerias h1 {color:darkBlue!important}
.vermelho h1,.vermelho h1>a{color:#903!important}
div.menu div a.altocontraste.ativada,div.menu div a.altocontraste:active{
	color:#000!important
}
tfoot td,
#subpagina.noticia article .datapub{
	color:#999
}
.degrade {
	color:#666!important
}
*.altocontraste {
	color:#ddd!important
}
div#loginintegrado *,div.aba ul a,#btnar {
	color:#fff
}
a.altocontraste,div.aba ul>li>a.altocontraste:hover,div.aba ul>li>a.ativada.altocontraste{
	color:#fff!important
}
div#prerodape h1{
	color:#336
}
ul.mapasite>li>ul>li>a {
	color:#103787
}
select,input {
	color:#435C81
}
a.ativada,a.ativada:hover,a.ativada:focus {
	color:#435C81!important
}
div[role=main] a {
	color:#6D85B4
}

/*
	[1.6]Decoração de texto
	_________________________________________________________
*/
a,.azul h1>a:hover,div.aba ul a:hover,div.aba ul a:focus,div.menu div a:hover,div.menu div a:focus,div.menu div a:active,li.ativo>a,li.ativo>a:hover,li.ativo>a:focus,#btnar:hover {
	text-decoration:none
}
a:hover,a:focus {
	text-decoration:underline	
}
a.altocontraste,div.aba ul>li>a.altocontraste:hover,div.aba ul>li>a.ativada.altocontraste{
	text-decoration:underline!important
}

/*
	[1.7]Estilo de lista
	_________________________________________________________
*/
nav *,ul[role=navigation] *,ul#breadcrumb li:first-child,ul#noticias,#slider ul,div#parcerias>div * {
	list-style:none
}
li.subconteudo {
	list-style:url(img/seta-direita.png)!important
}
ul#breadcrumb li {
	list-style:url(img/divisor-migalhas.png)
}
.verde ul{
	list-style:inside url(img/marcador_verde.png)
}
.amarelo ul{
	list-style:inside url(img/marcador_laranja.png)
}
.azul ul{
	list-style:inside url(img/marcador_azul.png)
}
.vermelho ul{
	list-style:inside url(img/marcador_vermelho.png)
}
li.ativo {
	list-style-image:url(img/seta-baixo.png)!important
}
li.subconteudo ul {
	list-style-image:none
}

/*
	[2] Especificações gerais
	[2.1]Displays
	_________________________________________________________
*/
.falar,li.subconteudo ul {
	display:none
}
a#logocabecalho,div#acessibilidade button,button#btpesquisa,a#btnar,a.degrade>span,a.borda>span,div#parcerias a,div.menu a,div.aba ul li,ul.mapasite,ul.mapasite a,a#logofooter {
	display:block
}
div#idiomas *,div#menuar>ul.mapasite,div#sobre * {
	display:inline
}
.borda,.degrade,#listaredes a,div#parcerias>div li,.grid>div,.modular>div,hr,ul.mapasite li{
	display:inline-block
}
div#loginintegrado a,div.aba ul a,p#contato {
	display:table-cell
}
@media aural,speech {
	.falar {
		display:inline
	}
}

/*
	[2.2]Alturas,larguras,margens,paddings e overflows
	_________________________________________________________
*/
hr,div#preconteudo,div#corpo,ul.mapasite,div#prerodape,div#rodape,div#listaacess,div#cabecalho,div#fundoazul,div#fundorodape,div#parcerias>div,.sitemap,.semborda.modular,#bannerrotativo .borda{
